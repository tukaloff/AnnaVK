<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tukaloff.annavk" >

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FriendsActivity"
            android:label="@string/title_activity_friends" >
        </activity>
        <activity
            android:name=".MessagesActivity"
            android:label="@string/title_activity_messages" >
        </activity>
        <activity
            android:name=".LastMessagesActivity"
            android:label="@string/title_activity_last_messages" >
        </activity>
        <activity
            android:name=".PhotoViewActivity"
            android:label="@string/title_activity_photo_view" >
        </activity>
        <activity
            android:name=".DownloadImageActivity"
            android:label="@string/title_activity_download_image" >
        </activity>
    </application>

</manifest>
